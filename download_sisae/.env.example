# Configuración de Base de Datos PostgreSQL
# Formato: postgresql://usuario:password@host:puerto/nombre_base_datos
DATABASE_URL=postgresql://postgres:tu_password@localhost:5432/sisae_db

# Clave secreta para sesiones (cambiar en producción)
SESSION_SECRET=sisae-secret-key-cambiar-en-produccion

# Puerto del servidor (opcional, por defecto 5000)
PORT=5000
